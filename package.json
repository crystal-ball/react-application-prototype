{
  "name": "@crystal-ball/react-app-prototype",
  "version": "1.0.0",
  "description": "Prototypical React application",
  "keywords": [
    "prototype",
    "react"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/react-app-prototype.git"
  },
  "engines": {
    "node": "14"
  },
  "scripts": {
    "commit": "cz",
    "start": "NODE_ENV=development webpack serve --mode=development",
    "start:instrument": "BABEL_ENV=instrument NODE_ENV=development webpack serve --mode=development",
    "start:docker": "NODE_ENV=development webpack serve --host=0.0.0.0 --mode=development",
    "start:storybook": "NODE_ENV=development start-storybook -p 6006",
    "build": "NODE_ENV=production webpack --mode=production",
    "build:stats": "NODE_ENV=production webpack --mode=production --json > stats.json",
    "build:storybook": "NODE_ENV=production build-storybook",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty . --ignore-pattern node_modules",
    "test:types": "tsc",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:acceptance": "docker-compose up --exit-code-from cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write . !CHANGELOG.md !coverage_cypress !coverage_jest !public"
  },
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "@mdx-js/react": "1.6.22",
    "@opentelemetry/api": "0.18.1",
    "@opentelemetry/context-zone": "0.18.2",
    "@opentelemetry/exporter-collector": "0.18.2",
    "@opentelemetry/instrumentation": "0.18.2",
    "@opentelemetry/instrumentation-document-load": "0.15.0",
    "@opentelemetry/instrumentation-fetch": "0.18.2",
    "@opentelemetry/resources": "0.18.2",
    "@opentelemetry/tracing": "0.18.2",
    "@opentelemetry/web": "0.18.2",
    "@reduxjs/toolkit": "1.5.1",
    "classnames": "2.3.1",
    "componentry": "3.3.1",
    "core-js": "3.10.1",
    "dux-routing": "0.5.0",
    "nanoid": "3.1.22",
    "numeral": "2.0.6",
    "path-to-regexp": "6.2.0",
    "prism-react-renderer": "1.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.3",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "svg-symbol-sprite-loader": "5.1.0"
  },
  "devDependencies": {
    "@crystal-ball/babel-base": "3.0.0",
    "@crystal-ball/commitizen-base": "2.10.0",
    "@crystal-ball/prettier-base": "2.6.0",
    "@crystal-ball/webpack-base": "11.1.0",
    "@cypress/code-coverage": "3.9.4",
    "@percy/cli": "1.0.0-beta.44",
    "@percy/cypress": "3.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@storybook/addon-essentials": "6.2.2",
    "@storybook/addon-links": "6.2.2",
    "@storybook/addons": "6.2.2",
    "@storybook/builder-webpack5": "^6.2.0-rc.13",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.2.2",
    "@storybook/theming": "6.2.2",
    "@testing-library/cypress": "7.0.5",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "5.1.1",
    "@types/jest": "26.0.22",
    "@types/mdx-js__react": "1.5.3",
    "@types/numeral": "2.0.1",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "6.8.0",
    "eslint-config-eloquence": "18.2.0",
    "eslint-plugin-mdx": "1.11.1",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "node-sass-utils": "1.1.3",
    "postcss-nested": "5.0.5",
    "react-is": "17.0.2",
    "react-refresh": "0.9.0",
    "tailwindcss": "2.0.4",
    "terser-webpack-plugin": "5.1.1",
    "typescript": "4.2.3"
  }
}
